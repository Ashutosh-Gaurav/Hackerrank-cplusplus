#include <iostream>
#include <iomanip>
using namespace std;

int main()
{
	int T;
	cin >> T;
	cout << setiosflags(ios::uppercase);
	cout << setw(0xf) << internal;
	while(T--)
    {
		double A;
        cin >> A;
		double B;
		cin >> B;
		double C;
		cin >> C;
        cout<<hex<<left<<showbase<<nouppercase;
        cout<<long(A)<<endl;
        cout<<dec<<setw(15)<<right<<showpos<<setfill('_')<<fixed<<setprecision(2);
        cout<<B<<endl;
        cout<<scientific<<setprecision(9)<<uppercase<<noshowpos;
        cout<<C<<endl;

	}
	return 0;

}
