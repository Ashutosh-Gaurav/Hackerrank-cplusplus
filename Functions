#include <iostream>
#include <cstdio>
using namespace std;


int max_of_four(int a, int b, int c, int d)
{
    int gr1,gr2,greatest;
    if (a>b)
    gr1=a;
    else
    gr1=b;

    if (c>d)
    gr2=c;
    else
    gr2=d;

    if (gr1>gr2)
    greatest=gr1;
    else
    greatest=gr2;

    return greatest;
}


int main() {
    int a, b, c, d;
    scanf("%d %d %d %d", &a, &b, &c, &d);
    int ans = max_of_four(a, b, c, d);
    printf("%d", ans);
    
    return 0;
}
